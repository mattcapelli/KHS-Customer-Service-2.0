<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Select Part:"
        android:id="@+id/textView3"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="(LEFT->RIGHT)"
        android:id="@+id/textView4"
        android:layout_gravity="center_horizontal" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2-Pole Circuit Breaker"
        android:id="@+id/button19"
        android:onClick="send_ac_circuit_protection4"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2-Pole Circuit Breaker"
        android:id="@+id/button20"
        android:onClick="send_ac_circuit_protection5"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2-Pole Circuit Breaker"
        android:id="@+id/button21"
        android:onClick="send_ac_circuit_protection6"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1-Pole Circuit Breaker"
        android:id="@+id/button28"
        android:onClick="send_ac_circuit_protection7"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1-Pole Circuit Breaker"
        android:id="@+id/button29"
        android:onClick="send_ac_circuit_protection8"/>
</LinearLayout>